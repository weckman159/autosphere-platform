{
  "rewrites": [
    {
      "source": "/legal/:path*",
      "destination": "/legal/:path*"
    },
    {
      "source": "/events/:id",
      "destination": "/events/[id]"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
